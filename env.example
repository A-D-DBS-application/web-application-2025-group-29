# Copy this file to .env and fill in real values

# Flask
SECRET_KEY=change-me-in-production

# Database (Supabase Postgres)
# Use the psql connection string from Supabase Dashboard → Settings → Database
# Example: postgresql://postgres:YOUR_DB_PASSWORD@db.PROJECT_REF.supabase.co:5432/postgres?sslmode=require
DATABASE_URL=postgresql://postgres:password@db.projectref.supabase.co:5432/postgres?sslmode=require

# Supabase client (optional if you use the REST/JS client elsewhere)
SUPABASE_URL=https://projectref.supabase.co
SUPABASE_KEY=your-anon-or-service-key


